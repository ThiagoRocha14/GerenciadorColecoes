<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="255.0" prefWidth="360.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.gerenciadorcolecoes.IndexController">
   <children>
      <Label alignment="CENTER" prefHeight="31.0" prefWidth="226.0" text="Gerenciador de Coleções" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin></Label>
      <Button fx:id="btnView" onAction="#switchToThird" text="Visualizar Itens">
         <VBox.margin>
            <Insets />
         </VBox.margin></Button>
      <Button fx:id="btnAdd" layoutX="30.0" layoutY="75.0" text="Adicionar Itens">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
      <Button fx:id="btnUpdate" layoutX="97.0" layoutY="98.0" text="Alterar Itens" />
      <Button fx:id="btnDel" layoutX="97.0" layoutY="121.0" text="Remover Itens">
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </Button>
      <Button fx:id="btnCad" layoutX="134.0" layoutY="219.0" onAction="#switchToSecondary" text="Cadastros" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
