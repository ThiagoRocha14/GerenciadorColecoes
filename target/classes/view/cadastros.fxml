<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CadastrosController">
   <children>
        <Label alignment="TOP_CENTER" text="Cadastros" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <font>
            <Font size="21.0" />
         </font>
      </Label>
      <TabPane prefHeight="250.0" prefWidth="376.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="50.0">
        <tabs>
          <Tab text="Status">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0">
                     <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="145.0" layoutY="14.0" prefHeight="16.0" prefWidth="87.0" text="Descrição" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="14.0">
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <TextArea fx:id="inputDescricaoStatus" layoutX="7.0" layoutY="42.0" prefHeight="123.0" prefWidth="363.0" AnchorPane.leftAnchor="7.0" AnchorPane.rightAnchor="6.0" AnchorPane.topAnchor="42.0" />
                        <Button fx:id="btnCadastrarStatus" alignment="CENTER" layoutX="270.0" layoutY="161.0" onAction="#cadastrarStatus" prefHeight="33.0" prefWidth="106.0" text="Cadastrar" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Colecao">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="145.0" layoutY="14.0" prefHeight="16.0" prefWidth="87.0" text="Descrição" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="14.0">
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <TextArea fx:id="txtDescricaoColecao" layoutX="6.0" layoutY="42.0" prefHeight="126.0" prefWidth="365.0" AnchorPane.leftAnchor="6.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="42.0" />
                          <Button fx:id="btnCadColecao" layoutX="270.0" layoutY="161.0" onAction="#cadastrarColecao" prefHeight="33.0" prefWidth="106.0" text="Cadastrar" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <Button fx:id="btnVoltar" mnemonicParsing="false" onAction="#switchToPrimary" text="Voltar" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" />
   </children>
</AnchorPane>
