<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="313.0" prefWidth="416.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddController">
   <children>
      <Label alignment="CENTER" prefHeight="31.0" prefWidth="226.0" text="Adicionar Item" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin></Label>
      <ImageView fx:id="imgView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="btnFoto" onAction="#selecionarImagem" text="Selecionar Foto">
         <VBox.margin>
            <Insets />
         </VBox.margin></Button>
         <Label alignment="CENTER" contentDisplay="CENTER" layoutX="145.0" layoutY="14.0" prefHeight="16.0" prefWidth="87.0" text="Descrição">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Label>
      <TextField fx:id="txtDescAdd" />
      <Button fx:id="btnAdd" layoutX="134.0" layoutY="219.0" text="Adicionar" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
