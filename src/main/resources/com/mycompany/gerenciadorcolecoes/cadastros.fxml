<VBox alignment="TOP_CENTER" prefHeight="313.0" prefWidth="416.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.gerenciadorcolecoes.CadastrosController">
    <children>
        <Label alignment="TOP_CENTER" text="Cadastros">
         <font>
            <Font size="21.0" />
         </font></Label>
      <TabPane prefHeight="250.0" prefWidth="376.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Status">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0">
                     <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="145.0" layoutY="14.0" prefHeight="16.0" prefWidth="87.0" text="Descrição" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="14.0">
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <TextArea fx:id="inputDescricaoStatus" layoutX="7.0" layoutY="42.0" prefHeight="113.0" prefWidth="363.0" AnchorPane.leftAnchor="7.0" AnchorPane.rightAnchor="6.0" AnchorPane.topAnchor="42.0" />
                        <Button fx:id="btnCadastrarStatus" alignment="CENTER" layoutX="270.0" layoutY="161.0" onAction="#switchToPrimary" prefHeight="33.0" prefWidth="106.0" text="Cadastrar" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Colecao">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="145.0" layoutY="14.0" prefHeight="16.0" prefWidth="87.0" text="Descrição" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="14.0">
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <TextArea fx:id="txtDescricaoColecao" layoutX="6.0" layoutY="42.0" prefHeight="81.0" prefWidth="365.0" AnchorPane.leftAnchor="6.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="42.0" />
                          <Button fx:id="btnCadColecao" layoutX="270.0" layoutY="161.0" onAction="#switchToPrimary" prefHeight="33.0" prefWidth="106.0" text="Cadastrar" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" />
                        <Label layoutX="14.0" layoutY="133.0" prefHeight="21.0" prefWidth="124.0" text="Total de Itens:" AnchorPane.leftAnchor="14.0" AnchorPane.topAnchor="133.0">
                           <font>
                              <Font size="17.0" />
                           </font>
                        </Label>
                        <TextField fx:id="txtTotal" layoutX="145.0" layoutY="130.0" prefHeight="26.0" prefWidth="223.0" AnchorPane.leftAnchor="145.0" AnchorPane.rightAnchor="8.0" AnchorPane.topAnchor="130.0" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
